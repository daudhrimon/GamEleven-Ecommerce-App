<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="12dp"
        android:height="12dp"
        android:gravity="center">
        <shape android:shape="oval">
            <stroke
                android:width=".3dp"
                android:color="#D0D0D0" />
        </shape>
    </item>
    <item
        android:width="7dp"
        android:height="7dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/selector_clr" />
        </shape>
    </item>
</layer-list>